{
  "$schema": "https://plugins.dprint.dev/dprint/dprint-plugin-json/latest/schema.json",
  "incremental": true,
  "lineWidth": 80,
  "typescript": {
    "quoteStyle": "alwaysSingle",
    "semiColons": "prefer",
    "quoteProps": "asNeeded",
    "module.sortImportDeclarations": "maintain"
  },
  "json": {},
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,yml,yaml,css,toml,html,md}"],
  "excludes": ["**/node_modules", "**/*-lock.yaml", "dist/", ".next"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.88.6.wasm",
    "https://plugins.dprint.dev/json-0.19.1.wasm",
    "https://plugins.dprint.dev/prettier-0.27.0.json@3557a62b4507c55a47d8cde0683195b14d13c41dda66d0f0b0e111aed107e2fe"
  ]
}
