{
  "$schema": "https://raw.githubusercontent.com/layooops/fsd-cli-kit/main/schemas/config-schema.json",
  "autogenerate": true,
  "namingConvention": {
    "file": "kebabCase",
    "folder": "kebabCase",
    "component": "pascalCase"
  },
  "globalSettings": {
    "scriptingLanguage": "typescript",
    "styles": {
      "cssFramework": "css-modules",
      "cssInJsFramework": "none",
      "cssPreprocessor": "css"
    },
    "framework": "react",
    "testing": {
      "enabled": false,
      "testFilePostfix": "test"
    },
    "stateManagement": {
      "type": "none"
    },
    "documentation": {
      "enabled": false,
      "documentTypes": []
    }
  }
}
